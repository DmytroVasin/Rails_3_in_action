<table class = 'table table-striped table-bordered' >
	<caption class='leadProjects'> Ticketee </caption>
		<tr>
			<th>Projects name</th>
			<th>Actions</th>
		</tr>
		<% @projects.each do |project| %>
			<tr>
				<td><%= link_to project.name, project_path(project.id) %></td>
				<td><%= link_to "Edit Project", edit_project_path(project.id) %></td>
			</tr>
		<% end %>

</table>
<%= link_to "New Project", new_project_path , class: 'btn btn-primary btn_back' %>