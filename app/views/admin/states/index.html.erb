<%= link_to "New State", new_admin_state_path, class: 'btn btn-primary btn_back' %>

<dl id='states' class="dl-horizontal">
	<% @states.each do |state| %>
  	<dt id='state_<%= state.id %>'><%= state.name %></dt>
		<dd id='state_<%= state.id %>'>
			<%= link_to "Make default", make_default_admin_state_path(state.id) %>
		</dd>
	<% end %>
</dl>
